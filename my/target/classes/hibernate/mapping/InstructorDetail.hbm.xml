<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
	<class table="instructor_detail" lazy="false" name="MavenHibernate.my.CRUD.InstructorDetail">
		<id column="id" type="int" name="id">
			<generator class="native"></generator>
		</id>
		<property name="youtubeChannel" type="string" column="youtube_channel"></property>
		<property name="hobby" type="string" column="hobby"></property>
	</class>
</hibernate-mapping>